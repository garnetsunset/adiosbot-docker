# Discord Bot Token (REQUIRED)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Gotify Configuration (OPTIONAL - leave empty to disable)
GOTIFY_URL=https://gotify.yourdomain.com
GOTIFY_TOKEN=your_gotify_app_token_here

# Auto-kick inactive members? (true/false)
AUTO_KICK=false

# Days of inactivity before taking action
INACTIVITY_CHECK_DAYS=30

# Check schedule in hours (168 = weekly, 24 = daily)
CHECK_SCHEDULE_HOURS=168